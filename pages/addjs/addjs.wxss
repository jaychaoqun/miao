#but{
  position: relative;
  width: 100%;
  height: 100%
}

.vie1{
  font-size: 14px;
  color: slategray;
  display: flex;
  justify-content: space-between;
  height: 60rpx;
}

.vie1 image{
  width: 20px;
  height: 20px;
  margin: 16rpx 20rpx;
}

.vie2{
  color: gray;
  font-size: 14px;
  display: flex;
  justify-content: space-around;
  padding: 10px 0;
  height: 60rpx;
}



.right1{
    display: flex;
    justify-content: space-around;
}

.right1 text{
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: gray;
}


.right2{
    background: #f8f8f8;
    height: 70rpx;
    display: flex;
}
.right2 view{
    flex:1;
    justify-content: center;
}
.right2 view text{
    line-height: 70rpx;
}
.right2 view image{
    width: 11px;
    height: 11px;
}


.right3{
    margin-left: 22%;
    font-size: 13px;
    color: gray;
}


.list{
    background: #fff;
}
.list .item{
    height: 70px;
    border-top:solid 1rpx #f2f2f2; 
    display: flex;
    flex-direction: row;
}

.list .item .left{
     width: 160rpx;
     text-align: center;
}

.list .item .left image{
    width: 45px;
    height: 45px;
    border-radius:40px;
    margin-top: 14px;
}

.list .item .right{
    flex:1;
    display: flex;
    flex-direction: column;
}
.list .item .right view{
    flex:1;
}
.row1{
    display: flex;
    padding-top: 10rpx;
}

.row1 .item1{
    font-size: 13px;
    flex:1;
}
.row1 .item2{
    font-size: 12px;
    color: #fd7213;
    margin-right: 5px;
    width:80px;
    text-align: right;
}


.row2 .item1{
    color:#9a9a9a;
    font-size: 13px;
}
.row2 .item2{
    color:#9a9a9a;
    font-size: 13px;
