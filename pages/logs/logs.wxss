page{
  background-color:#F0EFF5;
}
#but{
  position: relative;
  width: 100%;
  height: 100%
}

#vie1,#vie2,#lef{
  background-color: #fff;
}

#vie1{
  font-size: 14px;
  color: slategray;
  display: flex;
  justify-content: space-between;
}

#vie1 image{
  width: 20px;
  height: 20px;
  margin: 0 10px;
}

#vie2{
  color: gray;
  font-size: 14px;
  font-weight: bold;
  display: flex;
  justify-content: space-around;
  padding: 10px 0;
}


#lef{
  position: relative;
  height: 25px;
  line-height: 25px;
  font-size: 14px;
  color: gray;
  border-bottom:solid 1px gray; 
  padding-bottom: 5rpx;
}
#lef image{
  width: 20px;
  height: 20px;
}
.lef1{
  position: absolute;
  left: 5%;
}
.lef2{
  position: absolute;
  left: 20%;
}
.lef3{
  position: absolute;
  left: 75%;
}
.lef4{
  position: absolute;
  left: 92%;
}


#tab{
  position: relative;
  padding:0 5rpx;
}
#tab .tab1{
  height: 40px;
  line-height: 40px;
  border-bottom:solid 1px #ccc;
  font-size: 14px;
  color: gray;
  padding: 0 0 0 20rpx;
}
#tab .tab3{
  float: right;
  margin: 0 10px 0 0;
}
.butt{
  background: orange;
  font-size: 14px;
  color: #fff;
}
