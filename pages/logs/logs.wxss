page{
  background-color:#f3f3f3;
}
#but{
  display: flex;
  flex-direction: column;
  width: 100%
}

.vie1,.vie2,#lef{
  background-color: #fff;
}


#lef{
  position: relative;
  height: 25px;
  line-height: 80rpx;
  font-size: 14px;
  color: gray;
  /*background: #f8f8f8;*/
  background: #fff;
  padding-bottom: 5rpx;
  height: 80rpx;
  
}
#lef image{
  width: 25px;
  height: 25px;
  margin-top: 18rpx;
}
.lef1{
  position: absolute;
  left: 5%;
}
.lef2{
  position: absolute;
  left: 20%;
}
.lef3{
  position: absolute;
  left: 75%;
}
.lef4{
  position: absolute;
  left: 92%;
}


.board{
    flex: 1;
    display: flex;
    flex-direction: column; 
}

.board-item {
    flex:1;
    display: flex;
    cursor: pointer;
    border: 1rpx solid #eee;
    background: #fff;
    margin: 20rpx 30rpx 0 30rpx;
    padding: 30rpx;
}

.board-item text{
    flex: 1;
}

.board-item image{
    width: 40rpx;
    height: 40rpx;
}



.butt{
  position: fixed;
  z-index: 99;
  bottom:0;
  left:0;
  width:100%;
  height: 80rpx;
  background: orange;
  font-size: 14px;
  color: #fff;
}
