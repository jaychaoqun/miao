.but {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.vie1, .vie2, .lef {
  background-color: #fff;
}

.vie1 {
  font-size: 14px;
  color: slategray;
  display: flex;
  justify-content: space-between;
  height: 60rpx;
}

.vie1 image {
  width: 20px;
  height: 20px;
  margin: 16rpx 20rpx;
}

.vie2 {
  color: gray;
  font-size: 14px;
  display: flex;
  justify-content: space-around;
  padding: 10px 0;
  height: 60rpx;
}

.lef {
  position: relative;
  line-height: 80rpx;
  font-size: 14px;
  color: gray;
  background: #fff;
  padding-bottom: 5rpx;
  height: 80rpx;
}

.lef .lef3 .lef4 {
  width: 25px;
  height: 25px;
  margin-top: 18rpx;
}

.lef1 {
  position: absolute;
  left: 5%;
}

.lef2 {
  position: absolute;
  left: 20%;
}

.lef3 {
  position: absolute;
  left: 75%;
}

.lef4 {
  position: absolute;
  left: 92%;
}

.lef3 image, .lef4 image {
  width: 50rpx;
  height: 50rpx;
}

.board {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.board-item {
  flex: 1;
  display: flex;
  cursor: pointer;
  border: 1rpx solid #eee;
  background: #fff;
  margin: 20rpx 30rpx 0 30rpx;
  padding: 30rpx;
}

.board-item text {
  flex: 1;
}

.board-item image {
  width: 40rpx;
  height: 40rpx;
}

.butt {
  position: fixed;
  z-index: 99;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 80rpx;
  background: #fb8e4d;
  font-size: 14px;
  color: #fff;
}
